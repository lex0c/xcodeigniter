{
	"name": "lleocastro/xcodeigniter",
	"description": "My custom version of CodeIgniter",
	"type": "project",
	"homepage": "https://github.com/lleocastro/xcodeigniter",
	"license": "MIT",
	"support": {
		"forum": "http://forum.codeigniter.com/",
		"issues": "https://github.com/lleocastro/xcodeigniter/issues",
		"source": "https://github.com/lleocastro/xcodeigniter"
	},
	"require": {
		"php": ">=5.5.9",
		"symfony/var-dumper": "3.1.*",
		"vlucas/phpdotenv": "~2.2"
	},
	"require-dev": {
		"phpunit/phpunit": "5.6.*"
	},
	"autoload": {
		"psr-4": {
			"App\\": "/application/"
		}
	},
	"config": {
		"vendor-dir": "vendors",
		"optimize-autoloader": true
	}
}